    "accessRoles": {
        "workspaceAdmin": {
            "name": "Workspace Administrator",
            "description": "Full control of workspace resources",
            "permissions": [
                "read",
                "write",
                "delete",
                "manage_access",
                "manage_security"
            ]
        },
        "dataEngineer": {
            "name": "Data Engineer",
            "description": "Pipeline and data development",
            "permissions": [
                "read",
                "write",
                "execute_pipeline"
            ]
        },
        "dataScientist": {
            "name": "Data Scientist",
            "description": "Analysis and model development",
            "permissions": [
                "read",
                "write",
                "execute_notebook"
            ]
        },
        "dataAnalyst": {
            "name": "Data Analyst",
            "description": "Read-only access to data",
            "permissions": [
                "read",
                "execute_queries"
            ]
        }
    },
    "security": {
        "encryption": {
            "enabled": true,
            "keyVaultReference": "your-key-vault-url"
        },
        "networkIsolation": {
            "enabled": true,
            "allowedIpRanges": []
        }
    },
